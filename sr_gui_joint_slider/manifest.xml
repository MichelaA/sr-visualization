<package>
  <description brief="sr_gui_joint_slider">

     sr_gui_joint_slider is a rosgui plugin to change the position of the different joints.

  </description>
  <author>Toni Oliver</author>
  <license>GPL</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/sr_gui_joint_slider</url>
  <depend package="rospy"/>
  <depend package="rqt_gui"/>
  <depend package="rqt_gui_py"/>
  <depend package="pr2_mechanism_msgs"/>
  <depend package="sr_robot_msgs"/>
  <depend package="std_msgs"/>
  <depend package="sr_robot_lib"/>
  <depend package="sr_hand"/>

  <export>
    <rqt_gui plugin="${prefix}/sr_joint_slider_plugin.xml"/>
  </export>

</package>


